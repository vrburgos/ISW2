@model ISW2PDT5.departamento

@{
    ViewBag.Title = "Details";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Detalles del Departamento
        </div>
    </div>
    <div class="panel-body">
        <fieldset>
            <div class="form-group">
                <div class="editor-label">
                    ID del departamento
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.id_departamento)
                </div>
            </div>

            <div class="form-group">
                <div class="editor-label">
                    Nombre del Departamento
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.dep_nombre)
                </div>
            </div>
        </fieldset>
        <ul class="nav nav-pills">
            <li role="presentation"> @Html.ActionLink("Edit", "Edit", new { /* id=Model.PrimaryKey */ id = Model.id_departamento  })</li>
            <li role="presentation"> @Html.ActionLink("Back to List", "Index")</li>
        </ul>
    </div>
</div>